{
  "name": "idyll-hft-system",
  "version": "1.0.0",
  "description": "Idyll Intelligent Systems - Automated Nanosecond High-Frequency Trading Platform",
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "dev": "nodemon src/main.js",
    "build": "npm run build:core && npm run build:web",
    "build:core": "tsc -p tsconfig.json",
    "build:web": "cd web && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "benchmark": "node tests/performance/latency-benchmark.js",
    "simulate": "node src/simulation/market-simulator.js",
    "backtest": "node src/backtesting/backtest-engine.js",
    "deploy": "npm run build && node deploy/production-deploy.js",
    "monitor": "node src/monitoring/system-monitor.js",
    "risk-check": "node src/risk/risk-validator.js"
  },
  "keywords": [
    "hft",
    "high-frequency-trading",
    "algorithmic-trading",
    "ai",
    "machine-learning",
    "fintech",
    "nanosecond-trading",
    "ultra-low-latency"
  ],
  "author": "Idyll Intelligent Systems",
  "license": "Proprietary",
